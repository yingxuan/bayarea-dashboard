{
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/spend/today", "destination": "/api/spend" },
    { "source": "/api/spend/new-places", "destination": "/api/spend" },
    { "source": "/api/spend/enrich-place", "destination": "/api/spend" },
    { "source": "/api/spend/enrich-hours", "destination": "/api/spend" },
    { "source": "/api/spend/place-photo", "destination": "/api/spend" },
